<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hbt_mapping_xmls" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<value>com/szgr/vo/XnhUserVO.hbm.xml</value>
				<value>com/szgr/vo/XnhDerateVO.hbm.xml</value>
				<value>com/szgr/vo/KeyTablekeyvaluesVO.hbm.xml</value>
				<value>com/szgr/vo/DistrictVO.hbm.xml</value>
				<value>com/szgr/vo/XnhNoticeVO.hbm.xml</value>
				<value>com/szgr/vo/XnhNotice2empVO.hbm.xml</value>
				<!-- value>com/thtf/ynds/vo/SysCodSystemmessageVO.hbm.xml</value -->
			</list>

		</constructor-arg>
	</bean>

	<bean id="hbt_mapping_jars" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<value>classpath:hibernateConfig\szgr_authority_hbt.jar</value>
				<value>classpath:hibernateConfig\ynds_all_hbt.jar</value>
				<value>classpath:hibernateConfig\jnds-hbt.jar</value>
				<value>classpath:hibernateConfig\szgr_upload_hbt.jar</value>
			</list>
		</constructor-arg>
	</bean>

</beans>

